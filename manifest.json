{ 
   "name": "页面简化",
   "version": "1.3",
   "description": "页面，自动调整漫画大小适应窗口。", 
   "manifest_version":2,
   "icons": { "16": "favicon.png" ,"48": "icon.png","128": "icon.png"},
   "options_page": "options.html",
   "content_scripts": [
	{
	"matches": ["http://*.imanhua.com/*"],
	"css": ["comic.css"], 
	"run_at":"document_start"
	},
	{
	"matches": ["http://*.dm5.com/*"],
	"css": ["comic.css"], 
	"run_at":"document_start"
	},
	{
	"matches": ["http://*.dm5.com/m1*/"],
	"css": ["dm5.css"], 
	"run_at":"document_start"
	},
	{
	"matches": ["http://*.115.com/file/*"],
	"css": ["115.css"], 
	"run_at":"document_start"
	},
	{
	"matches": ["http://dl.dbank.com/*"],
	"css": ["dbank.css"], 
	"run_at":"document_start"
	},
	{
	"matches": ["http://*.weibo.com/*"],
	"css": ["weibo.css"], 
	"run_at":"document_start"
	},
	{
	"matches": ["http://video.sina.com.cn/v/*"],
	"css": ["sinavideo.css"], 
	"run_at":"document_start"
	},
	{
	"matches": ["http://video.sina.com.cn/p/*"],
	"css": ["sinavideo.css"], 
	"run_at":"document_start"
	},
	{
	"matches": ["http://*.deviantart.com/*"],
	"css": ["deviantart.css"], 
	"run_at":"document_start"
	}
	],
	"permissions": [
   	"storage"
 	]
}